.<%= @theme_name %>{
  color:<%= @body_text__default_code__primary_content %>
}
.<%= @theme_name %> a{
  color:<%= @blue %>
}
.<%= @theme_name %> table tr:nth-child(even){
  background-color:<%= @background_highlights %>
}
.<%= @theme_name %> table th{
  background:<%= @background %>
}
.<%= @theme_name %> table td,.<%= @theme_name %> table th{
  border-color:<%= @background_highlights %>
}
.<%= @theme_name %> blockquote{
  color:<%= @comments__secondary_contents %>
}
.<%= @theme_name %> hr{
  background-color:<%= @comments__secondary_contents %>
}
.<%= @theme_name %> code{
  background-color:rgba(<%= @background_highlights_rgba %>)
}
.<%= @theme_name %> svg{
  background-color:<%= @background %>
}
.<%= @theme_name %> .text-cell [style]{
  color:<%= @body_text__default_code__primary_content %>!important;
  background-color:transparent!important
}
